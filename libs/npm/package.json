{
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.5",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "build": "tailwindcss -i ../../static/css/tailwind.css -o ../../static/css/style.css",
    "build-watch": "tailwindcss -i ../../static/css/tailwind.css -o ../../static/css/style.css --watch",
    "build-min": "tailwindcss -i ../../static/css/tailwind.css -o ../../static/css/style.min.css --minify",
    "build-min-watch": "tailwindcss -i ../../static/css/tailwind.css -o ../../static/css/style.min.css --minify --watch",
    "build-all": "npm run build & npm run build-min"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.13.0",
    "@ckeditor/ckeditor5-build-classic": "^39.0.2",
    "alpinejs": "^3.13.0",
    "ckeditor5": "^39.0.2",
    "cropperjs": "^1.5.13",
    "htmx.org": "^1.9.5",
    "jquery": "^3.7.1"
  }
}
